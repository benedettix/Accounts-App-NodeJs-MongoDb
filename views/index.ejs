<%- include('./partials/head.ejs'); %>
    <div class="container">
        <div class="row">
            <div class="col-10 offset-1">
                <h3>Accounts</h3>
                <div class="row">
                    <div class="col-10 offset-1">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th scope="col">Firstname</th>
                                    <th scope="col">Lastname</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Phone</th>
                                </tr>
                            </thead>
                            <tbody>

                                <% for(let i=0;i<data.length;i++){ %>
                                    <tr>
                                        <td>
                                            <%= data[i].first_name %>
                                        </td>
                                        <td>
                                            <%= data[i].last_name %>
                                        </td>
                                        <td>
                                            <%= data[i].email %>
                                        </td>
                                        <td>
                                            <%= data[i].phone %>
                                        </td>
                                    </tr>
                                    <% } %>


                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <%- include('./partials/footer.ejs'); %>